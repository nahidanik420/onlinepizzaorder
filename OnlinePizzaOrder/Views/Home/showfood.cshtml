@model IEnumerable<OnlinePizzaOrder.Models.tbl_product>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Pizza Order</title>

    <link href="~/Content/pizzaorder/css/bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/pizzaorder/css/font-awesome.min.css" />
    <link href="~/Content/pizzaorder/css/custom.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/pizzaorder/css/style.css" />
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />


</head>
<body>


    <div id="wrapper">
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="adjust-nav">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#">
                        <img src="~/Content/pizzaorder/img/logo.png" />

                    </a>

                </div>

                <span class="logout-spn">
                    @Html.ActionLink("LogOut", "Logout", "Home", null, new { @class = "ht" })
                </span>
            </div>
        </div>

        <nav class="navbar-default navbar-side" role="navigation">
            <div class="sidebar-collapse">
                <ul class="nav" id="main-menu">

                    <li>
                        <a href="@Url.Action("Admin", "Home")"><i class="fa fa-desktop "></i>Dashboard</a>
                    </li>

                    <li>
                        <a href="@Url.Action("AdminFoodAdd", "Home")"><i class="fa fa-plus "></i>Add Pizza</a>
                    </li>
                    <li class="active-link">
                        <a href="@Url.Action("showfood", "Home")"><i class="fa fa-info-circle "></i>Pizza Details</a>
                    </li>
                    <li>
                        <a href="@Url.Action("showuser", "Home")"><i class="fa fa-users "></i>User Details</a>
                    </li>
                    <li>
                        <a href="@Url.Action("checkout", "Home")"><i class="fa fa-list-ol "></i>Order Details</a>
                    </li>

                </ul>
            </div>

        </nav>

        <div id="page-wrapper">
            <div id="page-inner">
                <div class="row">
                    <div class="col-lg-12">
                        <h2>Food Details</h2>
                    </div>
                </div>

                <hr />

                <div class="row pad-top">


                    <div class="col-md-1"></div>
                    <div class="col-md-10">
                        <div class="tblcart">
                            <table class="table">
                                <thead class="thead-dark">
                                    <tr>
                                        <th>FoodName</th>
                                        <th>Price</th>
                                        <th>Description</th>
                                        <th>Image</th>
                                        <th>Delete</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@item.pro_name</td>
                                            <td>@item.pro_price</td>
                                            <td>@item.pro_desc</td>
                                            <td>@item.pro_image</td>
                                            <td>&#10008;</td>
                                        </tr>
                                    }
                                    </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="col-md-1"></div>
                </div>
            </div>
        </div>
    </div>


</body>
</html>
